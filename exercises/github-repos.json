  "referenceCode": "async function getPopularRepositories(minStars: number): Promise<string[]> {\n  const results: string[] = [];\n  let page = 1;\n  let totalPages = 1;\n\n  while (page <= totalPages) {\n    const response = await fetch(`/api/repositories?page=${page}`);\n    const json = await response.json();\n    totalPages = json.total_pages;\n    \n    json.data\n      .filter((repo: any) => repo.stars >= minStars)\n      .forEach((repo: any) => results.push(repo.name));\n    \n    page++;\n  }\n  \n  return results;\n}"
}
